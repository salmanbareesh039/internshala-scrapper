"storageService": {
  "datasetRecordSchema": {
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "description": "Title of the internship"
      },
      "company": {
        "type": "string",
        "description": "Company offering the internship"
      },
      "location": {
        "type": "string",
        "description": "Location of the internship",
        "editor": "textfield"
      },
      "duration": {
        "type": "string",
        "description": "Duration of the internship"
      },
      "stipend": {
        "type": "string",
        "description": "Stipend offered for the internship"
      },
      "actively_hiring": {
        "type": "boolean",
        "description": "Whether the company is actively hiring"
      },
      "early_applicant": {
        "type": "boolean",
        "description": "Whether it's marked for early applicants"
      },
      "apply_link": {
        "type": "string",
        "description": "Link to apply for the internship"
      },
      "logo_url": {
        "type": "string",
        "description": "URL of the company logo"
      }
    }
  }
}
